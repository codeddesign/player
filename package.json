{
    "scripts": {
        "roll": "rollup -c rollup.config.js --environment build:production",
        "roll-debug": "rollup -c rollup.config.js --environment build:debug",
        "roll-dev": "rollup -c rollup.config.js --environment build:dev -w",
        "roll-dev-all": "npm run roll-dev & npm run roll-debug -- -w"
    },
    "devDependencies": {
        "rollup": "^0.34.13",
        "rollup-plugin-buble": "^0.13.0",
        "rollup-plugin-replace": "^1.1.1",
        "rollup-plugin-uglify": "^1.0.1",
        "rollup-watch": "^2.5.0"
    }
}
